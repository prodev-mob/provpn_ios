# Uncomment the next line to define a global platform for your project
platform :ios, '15.0'

def shared_pods
  use_frameworks!
  pod 'OpenVPNAdapter', :git => 'https://github.com/rahulsohaliya120/OpenVPNAdapter.git'

end

target 'ProVPN' do
  shared_pods
end

target 'TunnelProvider' do
  shared_pods
end
